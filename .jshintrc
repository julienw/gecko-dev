{
  "camelcase": false,
  "curly": true,
  "forin": false,
  "latedef": "nofunc",
  "newcap": false,
  "noarg": true,
  "nonew": true,
  "quotmark": "double",
  "undef": true,
  "unused": "vars",
  "strict": true,
  "trailing": true,
  "maxlen": 120,
  "varstmt": true,

  "eqnull": true,
  "esnext": true,
  "moz": true,
  "expr": true,
  "globalstrict": true,

  "maxerr": 1000,
  "regexdash": true,
  "laxcomma": true,
  "proto": true,

  "browser": true,
  "devel": true,
  "nonstandard": true,
  "worker": true,

  "-W078": true,
  "predef": [
    "dump",
    "Components",
    "Services",
    "XPCOMUtils"
  ]
}
